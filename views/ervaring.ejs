<!doctype html>
<html lang="en">

<head>
    <title>
        <%= locals.page %> - Prowser
    </title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" href="/assets/img/favicon.png" type="image/x-icon" />
    <link rel="stylesheet" href="/assets/css/main.min.css" />
    <link rel="stylesheet" href="/assets/css/styles.css" />
</head>

<body>
    <header class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
            <a class="navbar-brand" href="/">
                <img src="/assets/img/full_logo.png" alt="Bootstrap" width="60" height="54">
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
                aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <% locals.links.forEach(link=> { %>
                        <% if (locals.page==link.name) { %>
                            <li class="nav-item">
                                <a class="nav-link active" aria-current="page" href="<%= link.url %>">
                                    <%= link.name %>
                                </a>
                            </li>
                            <% } else { %>
                                <li class="nav-item">
                                    <a class="nav-link" href="<%= link.url %>">
                                        <%= link.name %>
                                    </a>
                                </li>
                                <% } %>
                                    <% }) %>
                </ul>
            </div>
        </div>
    </header>
    <main class="container mx-auto px-0">
        <div class="row py-5">
            <h1 class="text-center">Ervaring</h1>
        </div>
        <div class="row">
            <div
                class="col-11 col-md-8 col-lg-6 mx-auto my-1 ervaring-parent">
                <div class="mb-5 ervaring-slider animate__animated animate__fadeIn animate__faster">
                    <div class="d-flex justify-content-between align-items-center my-2">
                        <h2 class="mb-0">C#</h2>
                        <p class="mb-0">Volledig varend</p>
                    </div>
                    <div class="progress" role="progressbar" aria-label="Animated striped example" aria-valuenow="100"
                        aria-valuemin="0" aria-valuemax="100" animation-fill-time-ms="500" animation-delay-ms="0">
                        <div class="progress-bar progress-bar-striped progress-bar-animated"></div>
                    </div>
                </div>
                <div class="mb-5 ervaring-slider animate__animated animate__fadeIn animate__faster">
                    <div class="d-flex justify-content-between align-items-center my-2">
                        <h2 class="mb-0">Python</h2>
                        <p class="mb-0">Volledig varend</p>
                    </div>
                    <div class="progress" role="progressbar" aria-label="Animated striped example" aria-valuenow="100"
                        aria-valuemin="0" aria-valuemax="100" animation-fill-time-ms="500" animation-delay-ms="0">
                        <div class="progress-bar progress-bar-striped progress-bar-animated"></div>
                    </div>
                </div>
                <div class="mb-5 ervaring-slider animate__animated animate__fadeIn animate__faster">
                    <div class="d-flex justify-content-between align-items-center my-2">
                        <h2 class="mb-0">HTML, CSS</h2>
                        <p class="mb-0">Professional</p>
                    </div>
                    <div class="progress" role="progressbar" aria-label="Animated striped example" aria-valuenow="95"
                        aria-valuemin="0" aria-valuemax="100" animation-fill-time-ms="500" animation-delay-ms="0">
                        <div class="progress-bar progress-bar-striped progress-bar-animated"></div>
                    </div>
                </div>
                <div class="mb-5 ervaring-slider animate__animated animate__fadeIn animate__faster">
                    <div class="d-flex justify-content-between align-items-center my-2">
                        <h2 class="mb-0">Node.js (Javascript)</h2>
                        <p class="mb-0">Professional</p>
                    </div>
                    <div class="progress" role="progressbar" aria-label="Animated striped example" aria-valuenow="80"
                        aria-valuemin="0" aria-valuemax="100" animation-fill-time-ms="500" animation-delay-ms="0">
                        <div class="progress-bar progress-bar-striped progress-bar-animated"></div>
                    </div>
                </div>
                <div class="mb-5 ervaring-slider animate__animated animate__fadeIn animate__faster">
                    <div class="d-flex justify-content-between align-items-center my-2">
                        <h2 class="mb-0">MySQL</h2>
                        <p class="mb-0">Intermediate</p>
                    </div>
                    <div class="progress" role="progressbar" aria-label="Animated striped example" aria-valuenow="45"
                        aria-valuemin="0" aria-valuemax="100" animation-fill-time-ms="500" animation-delay-ms="0">
                        <div class="progress-bar progress-bar-striped progress-bar-animated"></div>
                    </div>
                </div>
                <div class="mb-5 ervaring-slider animate__animated animate__fadeIn animate__faster">
                    <div class="d-flex justify-content-between align-items-center my-2">
                        <h2 class="mb-0">PHP</h2>
                        <p class="mb-0">Beginnend</p>
                    </div>
                    <div class="progress" role="progressbar" aria-label="Animated striped example" aria-valuenow="25"
                        aria-valuemin="0" aria-valuemax="100" animation-fill-time-ms="500" animation-delay-ms="0">
                        <div class="progress-bar progress-bar-striped progress-bar-animated"></div>
                    </div>
                </div>
                
            </div>
        </div>
    </main>
    <script src="/assets/js/utils.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
        integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js"
        integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+"
        crossorigin="anonymous"></script>

    <script src="/assets/js/ervaring.js"></script>
</body>

</html>